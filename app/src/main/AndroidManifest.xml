<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="play.com.rhyme.meterialdemo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ToolBarActivity"/>
        <activity android:name=".NavigationViewActivity"/>
        <activity android:name=".FloatingActionButtonActivity"/>
        <activity android:name=".SnackbarActivity"/>
        <activity android:name=".CoordinatorLayoutActivity"/>
        <activity android:name=".AppBarLayoutActivity"/>
        <activity android:name=".CollapsingToolbarLayoutActivity"/>
        <activity android:name=".BottomNavigationViewActivity"/>
        <activity android:name=".TabLayoutViewActivity"/>
        <activity android:name=".CircularRevealActivity"/>
        <activity android:name=".BottomSheetDialogActivity"/>
        <activity android:name=".TextInputLayoutActivity"/>

    </application>

</manifest>